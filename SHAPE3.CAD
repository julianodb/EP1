# cubos
camera -g cam1 100 30 40 0 0 0 4 1 3 0 0   # カメラの設定
cube a 10 10 10                         # 立方体　10*10*10
clear -a
display cam1 a
wait 10
cube b 20 20 1                          # 直方体　20*20*1
clear -a
display cam1 b
wait 10
cube c 5 10 15                          # 直方体　5*10*15
clear -a
display cam1 c
